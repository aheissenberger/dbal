{
  "name": "ecotone/dbal",
  "license": "MIT",
  "homepage": "https://docs.ecotone.tech/",
  "type": "library",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Dariusz Gafka",
      "email": "dgafka.mail@gmail.com"
    }
  ],
  "keywords": [
    "dbal",
    "ecotone"
  ],
  "description": "Extends Ecotone with Dbal integration",
  "autoload": {
    "psr-4": {
      "Ecotone\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\Ecotone\\": [
        "tests",
        "vendor/ecotone/ecotone/tests"
      ]
    },
    "psr-0": {
      "FixtureAutoloadTest\\": "tests\\Messaging",
      "FixtureIncorrectNamespace\\": "tests\\Messaging"
    }
  },
  "require": {
    "ecotone/enqueue": "~1.8.1",
    "enqueue/dbal": "^0.10.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "behat/behat": "^3.3.1",
    "doctrine/dbal": ">=2.1",
    "doctrine/persistence": "^1.3|^2.0",
    "phpstan/phpstan": "^0.12",
    "doctrine/orm": "^2.0|^3.0",
    "doctrine/cache": "^1.0.0"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.8.1-dev"
    }
  }
}
